{
  "name": "Gdict",
  "description": "Lookup the selected text online",
  "version": "0.8.7",
  "permissions": ["contextMenus"],
  "background": {
    "persistent": false,
    "scripts": ["background.js", "gcookie.js"]
  },
  "manifest_version": 2,
  
  "browser_action": {
    "default_icon": "favicon.png",
    "default_title": "gdict history",
    "default_popup": "popup.html"
  }
}
