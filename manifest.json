{
  "name": "Gdict",
  "description": "Lookup the selected text online",
  "version": “0.9.6”,
  "permissions": ["contextMenus"],
  "background": {
    "persistent": false,
    "scripts": ["js/background.js", "js/gcookie.js"]
  },
  "manifest_version": 2,
  
  "icons": {
    "16": "img/favicon.png"
  },  
  "browser_action": {
    "default_icon": "img/favicon.png",
    "default_title": "gdict history",
    "default_popup": "popup.html"
  }
}
